{
  "name": "@spree/storefront-api-v2-sdk",
  "version": "4.5.4",
  "description": "Node module to easily integrate your JavaScript or TypeScript application with Spree API V2. You can create an entirely custom Storefront in JS/TS with this package including one page checkout, Single Page Apps, PWAs and so on",
  "engines": {
    "node": ">=14.15.4"
  },
  "main": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.esm.mjs",
  "scripts": {
    "prepublish": "npm run build",
    "build:esm": "rollup --config rollup.config.esm.js",
    "build:umd": "rollup --config rollup.config.umd.js",
    "build": "concurrently \"npm run-script build:esm\" \"npm run-script build:umd\"",
    "watch:esm": "rollup --watch --config rollup.config.esm.js",
    "watch:umd": "rollup --watch --config rollup.config.umd.js",
    "watch": "concurrently \"npm run-script watch:esm\" \"npm run-script watch:umd\"",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "docker-compose down && docker-compose up --exit-code-from cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spark-solutions/spree-storefront-api-v2-js-sdk.git"
  },
  "author": "Spark Solutions <we@sparksolutions.co> (https://sparksolutions.co)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spark-solutions/spree-storefront-api-v2-js-sdk/issues"
  },
  "homepage": "https://guides.spreecommerce.org/api/v2",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/node": "^14.14.37",
    "@types/qs": "^6.9.6",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "concurrently": "^6.2.0",
    "del": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1",
    "rollup": "^2.52.7",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-sizes": "^1.0.4",
    "rollup-plugin-terser": "^7.0.2",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.1.0",
    "tslib": "^1.14.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "qs": "^6.10.1"
  }
}
